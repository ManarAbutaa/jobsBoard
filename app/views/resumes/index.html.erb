<p style="color: green"><%= notice %></p>

<h1>Resumes</h1>

<table class="table table-striped">
<thead>
<tr>
<th>Cover letter</th>
<th>Title</th>
<th colspan="3"></th>
</tr>
</thead>

<tbody>
<% @resumes.each do |resume| %>
<tr>
<td><%= resume.cover_letter %> </td>
<td><%= resume.title %></td>

<td><%= link_to 'Show', resume%></td>

</td>

</tr>
<% end %>
</tbody>
</table>

